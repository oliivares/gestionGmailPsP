<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">

    <TableView fx:id="tablaCorreos">
        <columns>
            <TableColumn fx:id="columnaAsunto" text="Asunto" prefWidth="400"/>
            <TableColumn fx:id="columnaEtiqueta" text="Etiqueta" prefWidth="400"/>
        </columns>
    </TableView>

    <!-- Botón para volver a iniciar sesión -->
    <Button fx:id="btnReiniciarSesion" text="Reiniciar Sesión" />

</VBox>
